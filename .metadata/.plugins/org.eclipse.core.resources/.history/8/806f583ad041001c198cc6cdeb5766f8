#ifndef API_INC_API_DEBOUNCE_H_
#define API_INC_API_DEBOUNCE_H_

#include <stdio.h>

// Nuevo tipo de datos enumarado llamado statusMEF_t
typedef enum{
	BUTTON_UP,
	BUTTON_FALLING,
	BUTTON_DOWN,
	BUTTON_RISING
} statusMEF_t;

// Variable de estado (global)
statusMEF_t currentStatus;

// Prototype functions
void initMEF(void);
void updateMEF(void);


#endif /* API_INC_API_DEBOUNCE_H_ */
